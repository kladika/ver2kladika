{
  "name": "angular-site-template",
  "version": "1.0.0",
  "author": "https://angular-templates.io",
  "contributors": [
    "Dayana <dayana@startapplabs.com>",
    "Fermín <fermin@startapplabs.com>",
    "Agustín <agustin@startapplabs.com>"
  ],
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint angular-site-template",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-site-template:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@angular/animations": "6.0.9",
    "@angular/common": "6.0.9",
    "@angular/compiler": "6.0.9",
    "@angular/core": "6.0.9",
    "@angular/forms": "6.0.9",
    "@angular/http": "6.0.9",
    "@angular/platform-browser": "6.0.9",
    "@angular/platform-browser-dynamic": "6.0.9",
    "@angular/platform-server": "6.0.9",
    "@angular/router": "6.0.9",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@ngx-loading-bar/http-client": "^2.1.1",
    "@ngx-loading-bar/router": "^2.1.1",
    "bootstrap": "^4.1.2",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "intersection-observer": "^0.5.0",
    "moment": "^2.24.0",
    "ngx-bootstrap": "^3.0.1",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.9",
    "@angular/language-service": "6.0.9",
    "@types/node": "^8.0.30",
    "codelyzer": "^4.0.2",
    "cpy-cli": "^1.0.1",
    "express": "^4.15.2",
    "http-server": "^0.10.0",
    "pre-commit": "^1.2.2",
    "reflect-metadata": "^0.1.10",
    "rxjs-tslint": "^0.1.5",
    "ts-loader": "^4.4.2",
    "tslint": "^5.7.0",
    "typescript": "2.7.2",
    "webpack-cli": "^3.0.8"
  }
}
